{
  "name": "release-pr-label-version",
  "version": "1.0.0",
  "description": "> release prs with labels",
  "type": "module",
  "private": true,
  "engines": {
    "node": "24"
  },
  "scripts": {
    "tsc:check": "pnpm -r tsc:check",
    "build": "pnpm -r build",
    "lib:build": " pnpm --filter lib build",
    "lib:tsc:check": "pnpm --filter lib tsc:check",
    "labler:build": "pnpm --filter labler build",
    "labler:tsc:check": "pnpm --filter labler tsc:check",
    "labler-release:build": "pnpm --filter labler-release build",
    "labler-release:tsc:check": "pnpm --filter labler-release tsc:check",
    "labler-validator:build": "pnpm --filter labler-validator build",
    "labler-validator:tsc:check": "pnpm --filter labler-validator tsc:check",
    "test": "vitest run --pass-with-no-tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpbnetley/release-pr-label-version.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jpbnetley/release-pr-label-version/issues"
  },
  "homepage": "https://github.com/jpbnetley/release-pr-label-version#readme",
  "packageManager": "pnpm@10.28.1+sha512.7d7dbbca9e99447b7c3bf7a73286afaaf6be99251eb9498baefa7d406892f67b879adb3a1d7e687fc4ccc1a388c7175fbaae567a26ab44d1067b54fcb0d6a316",
  "dependencies": {
    "@actions/core": "^2.0.2",
    "@actions/github": "^7.0.0",
    "@types/node": "^20.19.30",
    "tsdown": "^0.19.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
